    <string name="preferences_upload_enabled_title">Sofort-Upload aktivieren</string>
    <string name="preferences_upload_enabled_summary">Aktiviere den Live-Upload zur CellMapper Website.  Datenvolumen wird dafür benötigt, aber die Ergebnissen sind am genausten.\n Du kannst die Daten aber auch später hochladen.</string>

    <string name="preferences_upload_auto_wifi_enabled_title">WLAN Auto-Upload</string>
    <string name="preferences_upload_auto_wifi_enabled_summary">Läd automatisch neue Daten hoch, wenn WLAN verfügbar ist.</string>

    
    <string name="preferences_cell_change_vibrate_title">Vibration bei Änderung der Zellen-ID </string>
    <string name="preferences_cell_change_vibrate_summary">Vibration als Benachrichtigung für Änderung der Zellen-ID </string>
    
    <string name="preferences_cell_change_sound_enabled_title">Benachrichtigungston bei Zellen-ID Änderung</string>
    <string name="preferences_cell_sound_enabled_summary">Spielt Benachrichtigungston bei Zellen-ID Änderung</string>
    
    <string name="preferences_start_on_boot_title">Autostart</string>
    <string name="preferences_start_on_boot_summary">Startet CellMapper beim Neustart des Handys</string>
    
    <string name="preferences_override_volume_title">Deaktivierung der Lautstärketasten</string>
    <string name="preferences_override_volume_summary">Deaktiviert die Lautstärketasten und verhindert das unbeabsichtigte Drücken</string>   

    <string name="preferences_use_imperial_title">Imperiale Einheiten anzeigen</string>
    <string name="preferences_use_imperial_summary">Zeigt Geschwindigkeit in MPH statt in Km/h. Höhe in Ft statt Meter</string>

    <string name="preferences_write_csv_file_title">CSV-Datei erstellen</string>
    <string name="preferences_write_csv_file_summary">Erstellt ein CSV-Datei für spätere Nutzung</string>   
    
    <string name="preferences_map_settings">Karteneinstellungen</string>
    
    <string name="preferences_display_towers_title">Zeige Sendemasten auf der Karte</string>
    <string name="preferences_display_towers_summary">Zeigt oder Versteckt Sendemasten auf der Karte</string>   
    
    <string name="preferences_show_low_accuracy_title">Zeige Sendemasten mit niedriger Genauigkeit</string>
    <string name="preferences_show_low_accuracy_summary">Zeigt oder versteckt Sendemasten mit unzureichenden Daten.</string>   
    
    <string name="preferences_advanced_settings">Weitere Einstellungen</string>
    
    <string name="preferences_newcellapi_enabled_title">Nutze die neue Android 4.2 Cell ID API</string>
    <string name="preferences_newcellapi_enabled_summary">Die neue Zellen API für Android 4.2 und neuer wird genutzt. Leder haben viele Hersteller (inkl. Google) die neue API nicht implementiert. So wird es meisten nicht funktionieren. App-Neustart vorausgesetzt.</string>       

    <string name="preferences_check_gps_age_title">Prüfe GPS-Fix Alter</string>
    <string name="preferences_check_gps_age_summary">Erhöht Datengenauigkeit in Tunneln und dicht besiedelten Gebieten. Auschalten, wenn \'GPS Fix zu alt\' angezeigt wird auch wenn dies nicht zutrifft.</string>       

    <string name="preferences_record_interval_title">Kleinster Aufzeichnungsinterval</string>
    <string name="preferences_record_interval_summary">Kleinster Sekundenabstand zwischen Daten-Punkten</string>       

    <string name="preferences_keep_screen_on_title">Bildschirm angschaltet lassen</string>
    <string name="preferences_keep_screen_on_summary">Kann nur ausgeschaltet werden wenn die Android 4.2 Cell ID API aktiviert ist.</string>

    <string name="preferences_keep_screen_on_brightness_title">Keep Screen On Brightness</string>
    <string name="preferences_keep_screen_on_brightness_summary">How bright to keep the screen when the phone attempts to go to sleep.</string>

    <string name="preferences_record_neighbours_title">Zeichne Nachbarzellen auf</string>
    <string name="preferences_record_neighbours_summary">Zeichnet Infos von Nachbarzellen auf. Sehr ungenau, außer dein Gerät untertützt die 4.2 CellID API.</string>       

    <string name="preferences_suppress_errors_title">Unterdrücke Fehler</string>
    <string name="preferences_suppress_errors_summary">Versteckt alle Fehler Pop-Ups, außer sie sind wichtig.</string>       

    <string name="preferences_upload_batch_size_title">Upload Größe</string>
    <string name="preferences_upload_batch_size_summary">Größe der Daten-Punkte, die pro Abfrage des Serves hochgeldaen werden.  Stelle eine kleinere Zahl ein, wenn der Upload öfters fehlschlägt.</string>       

    <string name="preferences_is_wear_enabled_title">Android Wear Benachritigungen aktivieren</string>
    <string name="preferences_is_wear_enabled_summary">Kontrolliert Android Wear Benachrichtigungen.  Neustart der App benötigt.</string>

    <string name="preferences_hide_unknown_cells_title">Verstecke Unbekannte Zellen</string>
    <string name="preferences_hide_unknown_cells_summary">Versteckt Zellen mit unbekannter Zellen-ID auf dem Hauptbildschirm.</string>

    <string name="preferences_read_logcat_title">Liest den Logcat von Samsung/Sony Field Test Daten (root)</string>
    <string name="preferences_read_logcat_summary">Ermöglicht das Lesen des Logcats von Samsung/Sony Field Test Mode Daten.  Do not enable this if you are using the screen reader method.  On newer phones, field test must run in foreground or dual-window mode.  Requires root for Android 4.1+ and app restart when changed.</string>

    <string name="preferences_read_logcat_qualcomm_title">Lese Qualcomm LTE Daten (root)</string>
    <string name="preferences_read_logcat_qualcomm_summary">Liest die LTE-Frequenzen auf Qualcomm-Modems.</string>

    <string name="preferences_read_logcat_qualcomm_device_title">Qualcomm-Modem Gerät</string>
    <string name="preferences_read_logcat_qualcomm_device_summary">Das ist das Modem das CellMapper benutzen wird.  Normalerweise, /dev/smd11.  Neustart der App benötigt.</string>


    <string name="preferences_estimate_band_title">Erkenne Frequenzbänder und Bandbreite</string>
    <string name="preferences_estimate_band_summary">Daten werden vom Server abgefragt, um GSM/UMTS/LTE Bänder/Bandbreite zu erkennen.  Benötigt Datenverbindung.</string>


    <string name="preferences_show_hex_values_title">Zeige Hex-Werte</string>
    <string name="preferences_show_hex_values_summary">Zeigt Hexadizimale Werte anstatt Dezimale Werte für die Zellen-IDs.</string>

    <!-- Misc errors -->
    <string name="gps_error_title">GPS Fehler</string>
    <string name="gps_error_body">GPS wird benötigt, damit die App richtig funktioniert.  Sonst sind manche Funktionen nicht verfügbar.</string>

    <string name="map_error_title">Kartenfelhler</string>
    <string name="map_error_body">Dein Handy unterstützt Googles Maps API v2 leider nicht.  Besuche die Webseite um deine Daten zu s.</string>

    <!-- Rest of prefs -->
    <string name="preferences_dual_sim_support_title">Dual SIM Untersützung</string>
    <string name="preferences_dual_sim_support_summary">Aktiviert/Deaktiviert Dual-SIM Unterstützung für Geräte, die die 4.2+ CellID API nicht unterstützen. Benötigt Neustart.</string>

    <string name="preferences_send_debug_title">Sende Debug Infomationen</string>
    <string name="preferences_send_debug_summary">Erzeugt eine Email mit Debug-Infomationen über dein Gerät für den Entwickler.  Wenn Funktionen auf deinem Gerät nicht funktionieren ist das sehr hilfreich.</string>


    <string name="blank_screen_press_back">Klicke auf Zurück um dieses Menü zu verlassen.</string>

    <string name="preferences_record2g_title">Aufzeichnen von 2G-Daten</string>
    <string name="preferences_record2g_summary">Aktiviert/Deaktiviert die Aufzeichnung von 2G (GSM, CDMA) Daten</string>

    <string name="preferences_record3g_title">Aufzeichnen von 3G-Daten</string>
    <string name="preferences_record3g_summary">Aktiviert/Deaktiviert die Aufzeichnung von 3G (UMTS) Daten</string>

    <string name="preferences_record4g_title">Aufzeichnen von 4G-Daten</string>
    <string name="preferences_record4g_summary">Aktiviert/Deaktiviert die Aufzeichnung von 4G (LTE) Daten </string>

    <string name="preferences_enable_geofencing_title">Enable location exclusion (geofencing)</string>
    <string name="preferences_enable_geofencing_summary">Enable/disable location exclusion when mapping.  Long tap on the map in order to set an area to exclude.  App restart required.</string>

    <string name="preferences_geofencing_radius_title">Geofencing radius (metres)</string>
    <string name="preferences_geofencing_radius_summary">Sets the exclusion radius for the above setting.  App restart required.</string>

    <string name="preferences_show_cells_with_frequency_only_title">Zeige nur Zellen mir Frequenz-Daten</string>
    <string name="preferences_show_cells_with_frequency_only_summary">Zeigt nur Zellen mit gültiger ARFCN/EARFCN/UARFCN.</string>

    <string name="stop_recording">Stoppe Aufzeichnung</string>
    <string name="start_recording">Starte Aufzeichnung</string>

    <string name="accessibility_description">Liest die Field-Test-Mode App Daten ohne Root.  Benötigt 4.2+ CellID API für Samsung Geräte.</string>
    <string name="battery_optimization_error">Der Sofort-Upload funktioniert nicht bis die "Batterie-Optimierung" ausgeschaltet ist und CellMapper im Hintergrund läuft. Um das zu ermöglichen gehe in die Android-Einstellungen > Akku > Akku-Optimierung (Untermenü) und stelle sicher, dass CellMapper als "Nicht Optimiert" unter "Alle Apps" eingestellt ist.</string>



